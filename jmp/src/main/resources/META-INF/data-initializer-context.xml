<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
                http://www.springframework.org/schema/beans
                http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="sellerInitializer" class="com.epam.jmp.spring.initializer.SellerInitializer">
        <property name="sellerMap">
            <map>
                <entry key="sellerId" value="sellerValue" />
            </map>
        </property>
    </bean>

    <bean id="proposalInitializer" class="com.epam.jmp.spring.initializer.ProposalInitializer"
          autowire="byName"/>

    <bean id="productInitializer" class="com.epam.jmp.spring.initializer.ProductInitializer">
        <constructor-arg type="com.epam.jmp.spring.Param" ref="param" />
    </bean>

    <bean id="dataInitializer" class="com.epam.jmp.spring.initializer.DataInitializer"
          init-method="init" />
</beans>